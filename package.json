{
  "name": "is-js-object",
  "version": "1.0.0",
  "description": "checks the given value is a js object",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover tape test.js",
    "jshint": "jshint -c .jshintrc *.js",
    "code-style": "jscs -p google *.js",
    "check-coverage": "istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "open-coverage": "open coverage/lcov-report/index.html"
  },
  "files": [
    "LICENSE",
    "README.md",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquimserafim/is-js-object.git"
  },
  "keywords": [
    "verify",
    "check",
    "js",
    "object"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/is-js-object/issues"
  },
  "homepage": "https://github.com/joaquimserafim/is-js-object",
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jscs": "^1.11.0",
    "jshint": "^2.6.0",
    "pre-commit": "^1.0.2",
    "tape": "^3.5.0"
  },
  "pre-commit": [
    "jshint",
    "code-style",
    "test",
    "check-coverage"
  ]
}
